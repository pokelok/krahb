                                            end
                                        end
                                    end
                                else
                                    topos(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                    MagnetDought = false
                                    if game:GetService("ReplicatedStorage"):FindFirstChild("Cookie Crafter [Lv. 2200]") then
